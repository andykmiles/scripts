alias python='docker run -it --rm --name pysys -v "$PWD":/usr/src/sys -w /usr/src/sys pysys python'
alias putup='docker run -it --rm --name pysys -v "$PWD":/usr/src/sys -w /usr/src/sys pysys putup'
alias ansible='docker run -it --rm --name pysys -v "$PWD":/usr/src/sys -w /usr/src/sys pysys ansible'
alias poetry='docker run -it --rm --name pysys -v "$PWD":/usr/src/sys -w /usr/src/sys pysys poetry'
alias tmux='tmux -2'
alias t='tmuxinator start andy'
